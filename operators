#average calculation

a=int(input())
b=int(input())
c=int(input())
d=int(input())
e=int(input())
print((a+b+c+d+e)/5)


#fencing ground

l=int(input())
b=int(input())
p=2*(l+b)
a=l*b
print("required length is %d m"%p)
print("required quantity of carpet is %d sqm"%a)


#splitting into teams

n=int(input())
a=int(input())
b=n//a
c=n%a
print(f'the number of students in each team is {b} and left out is {c}')
